(define-object VBox
  (in-module "Gtk")
  (parent "GtkBox")
  (c-name "GtkVBox")
  (gtype-id "GTK_TYPE_VBOX")
)

(define-function gtk_vbox_new
  (is-constructor-of "GtkVBox")
  (c-name "gtk_vbox_new")
  (caller-owns-return #t)
  (return-type "GtkWidget*")
  (parameters
    '("gboolean" "homogeneous")
    '("gint" "spacing")
  )
)


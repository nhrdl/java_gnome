(define-object DOMHTMLParagraphElement
  (in-module "WebKit")
  (import-header "webkit2/webkit2.h")
  (import-header "webkitdom/webkitdom.h")
  (import-header "webkitdom/webkitdom.h")
  (parent "DOMHTMLElement")
  (c-name "WebKitDOMHTMLParagraphElement")
)

(define-method get_align
  (of-object "WebKitDOMHTMLParagraphElement")
  (c-name "webkit_dom_html_paragraph_element_get_align")
  (return-type "gchar*")
  (parameters
  )
)

(define-method set_align
  (of-object "WebKitDOMHTMLParagraphElement")
  (c-name "webkit_dom_html_paragraph_element_set_align")
  (return-type "none")
  (parameters
    '("gchar*" "value")
  )
)


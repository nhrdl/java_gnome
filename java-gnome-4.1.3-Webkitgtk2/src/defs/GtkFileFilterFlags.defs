    '("filename" "GTK_FILE_FILTER_FILENAME")
    '("uri" "GTK_FILE_FILTER_URI")
    '("display-name" "GTK_FILE_FILTER_DISPLAY_NAME")
    '("mime-type" "GTK_FILE_FILTER_MIME_TYPE")
  )
)

